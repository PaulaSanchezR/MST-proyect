
<div class="col-md-12 "><h1>USERS</h1></div>
  <br><br><br>
  <hr class="colorgraph">
<div class="container">
  <table id="table" class="col-lg-12 table table-striped">
    <thead>
      <tr>
        <th><h2>Name</h2></th>
        <th><h2>Last Name</h2></th>
        <th><h2>Username</h2></th>
        <th><h2>Privilage</h2></th>
        <th><h2>Active</h2></th>
        <th><h2>Edit</h2></th>
      </tr>
    </thead>
  {{#each listUser}}
    <tr>
      <th>{{this.name}}</th>
      <th>{{this.lastName}}</th>
      <th>{{this.username}}</th>
      <th>{{this.privilage}}</th>
      <th>{{this.active}}</th>
      <form action ="/edit/{{this._id}}" method="POST">  
      <th><button type="submit">Edit</button></th>
    </form>
    </tr>
  {{/each}}
  </table>

</div>